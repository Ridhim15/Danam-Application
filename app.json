{
  "expo": {
    "name": "Danam",
    "slug": "danam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6A5ACD"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arpit23.danam"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon.png",
        "backgroundColor": "#6A5ACD"
      }
    },
    "web": {
      "favicon": "./assets/icons/icon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
